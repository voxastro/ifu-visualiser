{
  "name": "ifu-visualiser",
  "version": "0.0.1",
  "description": "Interactive visualizer for astronomical panoramic (IFU) spectral surveys data",
  "productName": "IFU-Visualiser",
  "author": "Ivan Katkov <ik52@nyu.edu>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./*",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "build": "URL_API=https://api-ifu.voxastro.org quasar build",
    "dev": "URL_API=http://127.0.0.1:8080 quasar dev",
    "deploy": "vercel"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.21.1",
    "plotly.js-cartesian-dist-min": "^2.12.1",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0",
    "@quasar/quasar-app-extension-qmarkdown": "^2.0.0-beta.7",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^8.5.0",
    "prettier": "^2.5.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
